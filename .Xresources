!-- Xft settings !
!Xft*dpi: 96
!Xft*dpi: 192
!-- Using `xdpyinfo | grep -B 2 resolution` u can find res for ur monitor
!Xft*dpi: 220
Xft*dpi: 220

Xft*antialias: true
/* Xft*rgba: rgb */
Xft*hinting: true
/* Xft*hinting:                full */
Xft*hintstyle: hintslight
/* Xft.hintstyle:  hintfull */
Xft*autohint: false
Xft*lcdfilter: lcddefault

!-- URxvt !
/* URxvt.font: xft:Liberation Mono:size=11,\ */
/*             xft:Dejavu Sans Mono for Powerline:size=19,\ */
/*             xft:FontAwesome:size=9 */
/* URxvt.boldFont: xft:Liberation Mono:bold:size=11,\ */
/*                 xft:Dejavu Sans Mono for Powerline:size=19,\ */
/*                 xft:FontAwesome:size=9 */
! URxvt.font: xft:Consolas:size=11,\
/* URxvt*font: xft:Dejavu Sans Mono for Powerline:size=13 */
/* XTerm*faceName: xft:Dejavu Sans Mono for Powerline:size=13 */
/* URxvt*boldFont: xft:Dejavu Sans Mono for Powerline:size=13 */

URxvt*font: xft:Dejavu Sans Mono:size=11
URxvt*faceName: xft:Dejavu Sans Mono:size=11
XTerm*utf8: 2
URxvt.letterSpace: 1
URxvt.saveLines: 1000000
/* URxvt*saveLines:            32767 */
/* XTerm*saveLines:            32767 */
/* URxvt.depth: 32 */
/* URxvt.background: rgba:0000/0000/0200/c800 */

/* URxvt.iconFile:             /usr/share/icons/elementary/apps/24/terminal.svg */
/* URxvt.letterSpace:          0 */
/* URxvt.colorUL:              #AED210 */
/* URxvt.perl-ext:             default,url-select */
/* URxvt.keysym.M-u:           perl:url-select:select_next */
/* URxvt.url-select.launcher:  /usr/bin/firefox -new-tab */
/* URxvt.url-select.underline: true */
!
!-- Perl extensions !
URxvt.perl-ext-common: default,matcher,resize-font
!-- URxvt.perl-ext-common: ...,tabbed,...

!-- Change background/foreground colours !
URxvt.keysym.C-7: command:\033]11;#ffffff\007\033]10;black\007
URxvt.keysym.C-6: command:\033]11;#000000\007\033]10;white\007

!-- Change font size on the fly !
URxvt.keysym.C-minus: resize-font:smaller
URxvt.keysym.C-equal: resize-font:bigger
URxvt.iso14755: false
URxvt.iso14755_52: false

!-- Xterm !
