# Increase Volume
"amixer -c1 sset Master playback 5%+"
m:0x0 + c:123
XF86AudioRaiseVolume

# Decrease Volume
"amixer -c1 sset Master playback 5%-"
m:0x0 + c:122
XF86AudioLowerVolume

# Mute Sound
"amixer -c1 sset Master toggle"
m:0x0 + c:121
XF86AudioMute

# Decrease Keyboard Brightness
"kbdlight down"
m:0x0 + c:237
XF86KbdBrightnessDown

# Increase Keyboard Brightness
"kbdlight up"
m:0x0 + c:238
XF86KbdBrightnessUp

# Decrease Screen Brightness
"brightness -set -5%"
m:0x0 + c:232
XF86MonBrightnessDown

# Increase Screen Brightness
"brightness -set +5%"
m:0x0 + c:233
XF86MonBrightnessUp
